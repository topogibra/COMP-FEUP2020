import static ioPlus.printHelloWorld() void;
class Test {
    int[] A;
    int N;

	public static void main(String[] args) {
        Test test;

        test = new Test();
        test.init();
        test.sumT();
	}

	public void init() {
        A = new int[5];
        N = 5;
	}

	public int sumT() {
	    int sum;
        int i;
        int t1;

        sum = 0;
        i = 0;

        while (i < N) {
              t1 = A[i];
              sum = sum + t1;
              i = i + 1;
        }

        return sum;
	}
}